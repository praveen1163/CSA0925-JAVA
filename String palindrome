import java.util.*;
class S1
{
   public static void main(String args[])
   {
    String str="pavani";
String rev="";
char ch;
int n=str.length();
for(int i=n-1;i>=0;i--)
{
ch=str.charAt(i);
rev=rev+ch;
}
if(str.equals(rev))
{
System.out.println( rev + " is a palindrome");
}
else
{
System.out.println(rev + " is not a palindrome");
}
}
}
