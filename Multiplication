import java.util.*;
class Multiplication
{
public static void main(String[] args)
{
int m,n,i;
Scanner sc=new Scanner(System.in);
System.out.println("enter the M value");
m=sc.nextInt();
System.out.println("enter the N value");
n=sc.nextInt();
for(i=1;i<=n;i++)
{
System.out.println(i+"*"+m+"="+(i*m));
}
}
}
