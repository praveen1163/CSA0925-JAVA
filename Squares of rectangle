import java.util.*;
class Rectangle
{
public static void main(String[] args)
{
int length,breadth,area;
Scanner sc=new Scanner(System.in);
System.out.println("enter the length");
length=sc.nextInt();
System.out.println("enter the breadth");
breadth=sc.nextInt();
area=length*breadth;
System.out.println("area is:"+area);
}
}
