import java.util.*;
class Operator
{
public static void main(String arg[])
{
int a=15,b=20;
if(a>3 && b<10)
{
System.out.println("true"); 
}
else
{
System.out.println("false"); 
}
if(a<10 || b<10)
{
System.out.println("true"); 
}
else
{
System.out.println("false"); 
}
if(a!=b)
{
System.out.println("false"); 
}
else
{
System.out.println("true")Â 
}
}
}
